@page
@model BankManagerApp.Pages.NorthwindModel
@{
}

<h1>Alla Suppliers!</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Company Name</th>
            <th scope="col">Region</th>
        </tr>
    </thead>
    <tbody>
    
    @foreach (var supp in Model.Suppliers)
    {
        <tr>
            <td>@supp.Id</td>
            <td>@supp.CompanyName</td>
            <td>@supp.Region</td>
        </tr>

    }

    </tbody>
</table>